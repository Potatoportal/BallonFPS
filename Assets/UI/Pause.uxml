            </ui:VisualElement>
        </ui:VisualElement>
        <ui:Button text="Main Menu" parse-escape-sequences="true" display-tooltip-when-elided="true" name="MainMenuButton" style="align-self: auto; justify-content: flex-start; font-size: 20%;" />
    </ui:VisualElement>
</ui:UXML>
